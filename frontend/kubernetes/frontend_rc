apiVersion: v1
kind: ReplicationController
metadata:
  name: backend-api
spec:
  replicas: 2
  template:
    metadata:
      labels:
        app: backend-api
    spec:
      containers:
      - name: backend-api
        image: eu.gcr.io/demokubernetes/backend-api:0.4
        ports:
        - containerPort: 4567

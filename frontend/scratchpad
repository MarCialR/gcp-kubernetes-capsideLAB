gcloud container clusters create NAME
        [--cluster-ipv4-cidr CLUSTER_IPV4_CIDR] [--disk-size DISK_SIZE]
        [--enable-cloud-endpoints] [--enable-cloud-logging]
        [--enable-cloud-monitoring]
        [--machine-type MACHINE_TYPE, -m MACHINE_TYPE] [--network NETWORK]
        [--num-nodes NUM_NODES; default="3"] [--password PASSWORD]
        [--scopes SCOPE,[SCOPE,...]] [--subnetwork SUBNETWORK]
        [--username USERNAME, -u USERNAME; default="admin"] [--wait]
        [--zone ZONE, -z ZONE] [GLOBAL-FLAG ...]

gcloud container clusters create mysql --machine-type n1-standard-1 -num-nodes 1 --zone europe-west1-c --disk-size 10 
